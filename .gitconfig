[core]
    safecrlf = true
    excludesfile = .gitignore
    preloadindex = true

[diff]
    algorithm = histogram

[fetch]
    prune = true

[pull]
    rebase = false

[merge]
    ff = only

[push]
    default = current
    autoSetupRemote = true

[alias]
    st = status
    co = checkout
    br = branch
    ci = commit
    unstage = reset HEAD --
    last = log -1 HEAD

* text=auto
*.js text eol=lf
*.css text eol=lf
*.html text eol=lf
*.json text eol=lf
*.md text eol=lf
*.py text eol=lf
*.md linguist-documentation
